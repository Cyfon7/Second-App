    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

  </head>
  
  <body class="no-sidebar is-preload">

    <div id="page-wrapper">

      <%= render "share/header" %>

			<!-- Main -->
